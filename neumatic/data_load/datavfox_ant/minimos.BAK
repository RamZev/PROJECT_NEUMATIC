CLOSE TABLES ALL 

USE listamin
=AFIELDS(aLista)
CREATE CURSOR NuevaLista FROM ARRAY aLista

SELECT NuevaLista 
cArchivo = GETFILE("CSV","Archivos")
APPEND FROM (cArchivo) DELIMITED WITH  CHARACTER ';'

SUSPEND 

SELECT nuevaLista
SCAN 
	SCATTER MEMVAR 
	*----------------------------------- Si es un CAI que ya existe no se debe duplicar
	SELECT listamin
	DELETE for cai=m.cai
	
	SELECT nuevaLista
	INSERT INTO listamin FROM MEMVAR 

	SCAN WHILE cai=m.cai
		SCATTER MEMVAR 
		INSERT INTO listamin FROM MEMVAR 
	ENDSCAN 
	SKIP -1
ENDSCAN
